           echo "   x = 0" >> zion.rb
           echo "   10000000.times do |i|" >> zion.rb
           echo "   x = i + x" >> zion.rb
           echo "   end" >> zion.rb
           echo "end" >> zion.rb
           echo "fork" >> zion.rb
           echo "cpu_intensive_process" >> zion.rb
