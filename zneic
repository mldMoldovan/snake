 sh zneic -uninstall
 sudo rm -rf zneic
 cat zneic2 | sed "s/zneic/$_new/g" | sed "s/Z.N.E.I.C/$_converted/g" > $_new
 sudo rm -rf zneic2 ; sh $_new -qinstall ; sudo rm -rf $_new

elif [[ $_system == "OS/390" ]]; then

  _converted=$( echo $1 | rev | sed 's/./&./g' | awk '{print toupper($0)}' | sed 's/.$//')
  _new="$(echo $1 | rev )"


