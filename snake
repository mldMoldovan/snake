    arp -a | sed -n '1p'
    printf "$YELLOW"
    arp -a | grep $(ifconfig | grep inet | sed '1,2d' | awk '{print $3}')
    printf "$NC"
    arp -a | sed '1d'
    echo
    printf "$WHITE\n"
    date
    printf "$NC\n"

  elif [[ $_system == "GNU" ]]; then



