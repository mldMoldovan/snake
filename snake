    echo "   date" >> .bash_profile
    echo "}" >> .bash_profile
    echo "quietTextRestore(){" >> .bash_profile
    echo "   NC='\033[0m'" >> .bash_profile
    echo "   printf \"$NC\n\"" >> .bash_profile
    echo "}" >> .bash_profile
    echo " " >> .bash_profile
    source .bash_profile
