  elif [[ $_system == "Haiku" ]]; then
      printf "Not available on your OS$NC\n"
  elif [[ $_system == "$(uname -n)" ]]; then   #UNIX
      printf "Not available on your OS$NC\n"
  else
      printf "Not available on your OS$NC\n"
  fi

}


