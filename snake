_update(){

   mkdir foornal
   cd foornal
   git clone https://github.com/mldMoldovan/snake.git -q >/dev/null
   cd snake
   sudo chmod +x snake
   sudo cp snake /usr/local/bin
   cd .. ; cd ..
   sudo rm -rf foornal
   printf "$RED _updated$NC\n"


}


_brew(){



   if [ $1 == "html" ]; then

       if [ $2 == "?" ]; then
       nano .brew.html
       else
       nano brew.html
       fi

   elif [[ $1 == "css" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.css
     else
     nano brew.css
     fi
   elif [[ $1 == "c++" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.cpp
     else
     nano brew.cpp
     fi
   elif [[ $1 == "c" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.c
     else
     nano brew.c
     fi
   elif [[ $1 == "sh" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.sh
     else
     nano brew.sh
     fi
   elif [[ $1 == "_h" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.h
     else
     nano brew.h
     fi
   elif [[ $1 == "js" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.js
     else
     nano brew.js
     fi
   elif [[ $1 == "json" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.json
     else
     nano brew.json
     fi
   elif [[ $1 == "rb" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.rb
     else
     nano brew.rb
     fi
   elif [[ $1 == "swift" ]]; then
     if [ $2 == "?" ]; then
     nano .brew.swift
     else
     nano brew.swift
     fi
   elif [[ $1 == "magic" ]]; then
      _brew_magic
   else
       printf "$NEW_BLUE Brew$YELLOW Error$NC\n"
   fi


}


_brew_magic(){

   printf "\n"
   printf "$NEW_BLUE HTML / CSS / C++ / C / *SH / ^H$NC\n"
   printf "$NEW_BLUE JS / JSON / RB / SWIFT / *BAT / ^H$NC\n"
   printf "\n"


}



_real_summon(){

    _system="$(uname -s)"

    if [ $_system == "Darwin" ]; then
      printf "$RED Summoning $1...$NC\n"
      cd ~
      mkdir .$1 ; cd .$1/
      git clone https://github.com/mldMoldovan/$1 -q >/dev/null 2>&1
      cd ./$1
      sudo chmod +x $1 ; sudo cp $1 /usr/local/bin/
      cd .. ; cd .. ; rm -rf .$1
      printf "$RED Kolya is here $NC\n"
    elif [[ $_system == "Linux" ]]; then
      printf "$RED Not available on your system$NC\n"
    elif [[ $_system == "Minix" ]]; then
      printf "$RED Not available on your system$NC\n"
    elif [[ $_system == "OpenBSD" ]]; then
      printf "$RED Not available on your system$NC\n"
    elif [[ $_system == "Haiku" ]]; then
      printf "$RED Summoning $1...$NC\n"
      cd ~
      mkdir .$1 ; cd .$1/
      git clone https://github.com/mldMoldovan/$1 -q >/dev/null 2>&1
      cd ./$1
      chmod +x $1
      printf "$RED Kolya is here $NC\n"
    else
      printf "$RED Not available on your system$NC\n"
    fi


}

_summon(){



   if [ "$1" == "magic" ]; then
      printf "$RED Kolya $YELLOW Wilfred $GREEN Salazaar $BLUE Beria $WHITE Shinigami $NEW_BLUE Samael$NC\n"
   elif [[ "$1" == "kolya" ]]; then
      _real_summon kolya
   elif [[ "$1" == "wilfred" ]]; then
      printf "$YELLOW Summoning Wilfred...$NC\n"
      cd ~
      mkdir .wilfred ; cd .wilfred/
      git clone https://github.com/mldMoldovan/wilfred -q
      cd ./wilfred
      sudo cp wilfred /usr/local/bin ; sudo chmod +x wilfred
      cd .. ; cd .. ; rm -rf .wilfred
      printf "$YELLOW Wilfred is here $NC\n"
   elif [[ "$1" == "salazaar" ]]; then
      printf "$GREEN Summoning Salazaar...$NC\n"
      cd ~
      mkdir .salazaar ; cd .salazaar/
      git clone https://github.com/mldMoldovan/salazaar -q
      cd ./salazaar
      sudo cp salazaar /usr/local/bin ; sudo chmod +x salazaar
      cd .. ; cd .. ; rm -rf .salazaar
      printf "$GREEN Salazaar is here $NC\n"
   elif [[ "$1" == "beria" ]]; then
      printf "$BLUE Summoning Beria...$NC\n"
      cd ~
      mkdir .beria ; cd .beria/
      git clone https://github.com/mldMoldovan/beria -q >/dev/null
      cd ./beria
      sudo cp beria /usr/local/bin ; sudo chmod +x beria
      cd .. ; cd .. ; rm -rf .beria
      printf "$BLUE Beria is here $NC\n"
    elif [[ "$1" == "shinigami" ]]; then
      printf "$WHITE Summoning Shinigami...$NC\n"
      cd ~
      mkdir .shinigami ; cd .shinigami/
      git clone https://github.com/mldMoldovan/shinigami -q
      cd ./shinigami
      sudo chmod +x shinigami ; sudo cp shinigami /usr/local/bin
      cd .. ; cd .. ; sudo rm -rf .shinigami
      printf "$RED Shinigami is here $NC\n"
   else
      printf "$RED Summoning failed$NC\n"
   fi


}

_badblood(){

while true
do
    _blood
    sleep $1
done

}







