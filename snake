printf "         -e - engage\n"
printf "         --summon - Daemon summoning\n"
printf "         --morph - Network morphing\n"
printf "         --brew - Just like magic\n"
printf "\n"
printf "Examples: snake -p\n"
printf "          snake --summon Artyom\n"
printf "          snake --opaa\n"
printf "\n"


